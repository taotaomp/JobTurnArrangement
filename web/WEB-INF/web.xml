<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>MonthGetServlet</servlet-name>
        <servlet-class>cn.PApudding.Servlet.MonthGetServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MonthGetServlet</servlet-name>
        <url-pattern>/MonthGet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DayOffSetServlet</servlet-name>
        <servlet-class>cn.PApudding.Servlet.DayOffSetServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DayOffSetServlet</servlet-name>
        <url-pattern>/DayOffSetServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>PeopleSetServlet</servlet-name>
        <servlet-class>cn.PApudding.Servlet.PeopleSetServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PeopleSetServlet</servlet-name>
        <url-pattern>/PeopleSetServlet</url-pattern>
    </servlet-mapping>
</web-app>